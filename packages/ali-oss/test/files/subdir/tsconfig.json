{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "declaration": false,
    "inlineSourceMap": false,
    "noEmit": true,
    "noImplicitAny": false,
    "noImplicitThis": false,
    "noUnusedLocals": false,
    "sourceMap": false,
    "target": "ESNEXT"
  },
  "ts-node": {
    "esm": true,
    "preferTsExts": true,
    "transpileOnly": false
  },
  "include": [
    "**/*.ts"
  ],
  "exclude": [
    "asset/",
    "fixtures/",
    "node_modules*",
    "**/*.d.ts",
    "**/*.spec.ts"
  ]
}
